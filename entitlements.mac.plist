<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow incoming network connections (for AEM server ports) -->
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- Allow outgoing network connections -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Allow file system access for AEM installation and project files -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <!-- Allow access to Downloads folder (common location for AEM SDK) -->
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    
    <!-- Allow spawning child processes (for AEM instances) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation for Java processes -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow JIT compilation (needed for Java/AEM) -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow dynamic code generation -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
</dict>
</plist> 