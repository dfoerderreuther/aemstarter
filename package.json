{
  "name": "aem-starter",
  "productName": "AEM-Starter",
  "version": "1.0.2",
  "description": "AEM-Starter - Development environment for Adobe Experience Manager",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 electron-forge start",
    "dev:electron": "NODE_NO_WARNINGS=1 nodemon",
    "dev": "concurrently \"vite\" \"wait-on tcp:5173 && sleep 2 && npm run dev:electron\"",
    "clean": "rm -rf out/",
    "prebuild": "npm run clean",
    "build": "NODE_NO_WARNINGS=1 electron-forge package",
    "prebuild:signed": "npm run clean",
    "build:signed": "NODE_NO_WARNINGS=1 DEBUG=electron-forge:* electron-forge package --arch=arm64",
    "build:verbose": "NODE_NO_WARNINGS=1 DEBUG=electron-forge:*,electron-osx-sign:*,electron-notarize:* electron-forge package --arch=arm64",
    "make": "electron-forge make --arch=arm64",
    "make:verbose": "DEBUG=electron-forge:*,electron-osx-sign:*,electron-notarize:* electron-forge make --arch=arm64",
    "setup:signing": "node -e \"require('./scripts/setup-signing.js')()\"",
    "run:app": "open out/AEM-Starter-darwin-arm64/AEM-Starter.app",
    "test:app": "npm run build && npm run run:app",
    "test:signed": "npm run build:signed && npm run run:app",
    "test:pkg": "npm run make && echo 'PKG installer created! Install from out/make/'",
    "release": "npm run make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Dominik FÃ¶rderreuther",
    "email": "df@adobe.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@electron/notarize": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "electron": "36.2.1",
    "electron-notarize": "^1.2.2",
    "electronmon": "^2.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.4.19",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mantine/core": "^8.0.1",
    "@mantine/hooks": "^8.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@tabler/icons-react": "^3.33.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/uuid": "^10.0.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "adm-zip": "^0.5.10",
    "electron-squirrel-startup": "^1.0.1",
    "extract-zip": "^2.0.1",
    "monaco-editor": "^0.52.2",
    "node-pty": "^1.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "update-electron-app": "^3.1.1",
    "uuid": "^11.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zip-a-folder": "^3.1.9"
  }
}
